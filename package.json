{
  "name": "acge-gabon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:turbo": "next dev --turbo",
    "dev:fast": "next dev --turbo --port 3001",
    "dev:debug": "next dev --turbo --inspect",
    "dev:clean": "rimraf .next && rimraf node_modules/.cache && next dev --turbo",
    "dev:no-turbo": "next dev",
    "dev:safe": "next dev --port 3001",
    "build": "next build --turbo",
    "build:dev": "next build --turbo",
    "build:analyze": "ANALYZE=true next build --turbo",
    "start": "next start",
    "lint": "next lint",
    "setup:env": "node scripts/setup-env.js",
    "setup:env:force": "node scripts/setup-env.js --force",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f postgres",
    "build:prod": "NODE_ENV=production next build",
    "start:prod": "NODE_ENV=production next start -p $PORT",
    "migrate:prepare": "node scripts/migration/migrate-to-new-account.js",
    "migrate:validate": "node scripts/migration/validate-migration.js",
    "optimize:pre-migration": "node scripts/migration/optimize-before-migration.js",
    "clean:build": "rimraf .next out",
    "clean:cache": "rimraf .turbo node_modules/.cache",
    "fresh:install": "rimraf node_modules package-lock.json && npm install",
    "optimize:images": "node scripts/optimize-images.js",
    "analyze:bundle": "ANALYZE=true npm run build",
    "perf:audit": "npm run build && npm run start"
  },
